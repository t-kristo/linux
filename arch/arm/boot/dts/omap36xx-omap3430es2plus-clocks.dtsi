	clocks = <&sys_ck>, <&sys_ck>;
	ti,modes = <0x82>;
	reg-names = "control", "idlest", "autoidle", "mult-div1";
	reg = <0x48004d04 0x4>, <0x48004d24 0x4>, <0x48004d34 0x4>, <0x48004d4c 0x4>;
};

